{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Node",
    "declaration": true,
    "baseUrl":".",
    "paths": {
      "@src/*": [
        "src/*"
      ],
      "@packages/*": [
        "packages/*"
      ]
    },
    "declarationDir":"types"
  },
  
}