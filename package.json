{
  "name": "lin-view-ui",
  "version": "1.0.0",
  "description": "vue components library",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "unpkg": "lib/index.js",
  "style": "lib/theme-chalk/index.css",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "keywords": [
    "vue",
    "UI",
    "Component"
  ],
  "typings": "packages/types/index.d.ts",
  "homepage": "https://github.com/c10342/lin-view-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/c10342/lin-view-ui"
  },
  "bugs": {
    "url": "https://github.com/c10342/lin-view-ui/issues"
  },
  "files": [
    "lib",
    "packages"
  ],
  "author": "c10342",
  "license": "MIT",
  "scripts": {
    "dev:docs": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.docs.js",
    "dev:examples": "cross-env NODE_ENV=development target=examples webpack-dev-server --config ./build/webpack.docs.js",
    "build:docs": "cross-env NODE_ENV=production webpack --config ./build/webpack.docs.js",
    "upload:docs": "npm run build:docs && node ./scripts/qiniu.js",
    "build:utils": "node ./build/build.utils.js",
    "build:locale": "node ./build/build.locale.js",
    "build:mixins": "node ./build/build.mixins.js",
    "build:component": "node ./build/build.component.js",
    "build:all": "node ./build/build.all.js",
    "build": "npm run build:utils && npm run build:locale && npm run build:mixins && npm run build:component && npm run build:all",
    "bootstrap": "lerna bootstrap",
    "commit": "git cz",
    "add": "node ./scripts/componentTemplate.js",
    "lint": "eslint --ext .js,.vue,.jsx ./",
    "lint-fix": "eslint --fix --ext .js,.vue,.jsx ./",
    "lint:style": "stylelint **/*.{html,vue,css,sass,scss}",
    "lint:style-fix": "stylelint **/*.{html,vue,css,sass,scss} --fix",
    "test-watch": "jest --watch",
    "test": "jest --coverage && codecov",
    "lint-staged": "lint-staged --config lint-staged.config.js",
    "commitlint": "commitlint --config commitlint.config.js -e -V",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  },
