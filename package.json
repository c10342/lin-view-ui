{
  "name": "lin-view-ui",
  "version": "1.0.26",
  "description": "vue components library",
  "author": "c10342",
  "private": false,
  "main": "lib/index.js",
  "module": "lib/index.js",
  "unpkg": "lib/index.js",
  "license": "MIT",
  "keywords": [
    "vue",
    "UI",
    "Component"
  ],
  "homepage": "https://github.com/c10342/lin-view-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/c10342/lin-view-ui"
  },
  "files": [
    "lib",
    "packages",
    "utils",
    "src"
  ],
  "scripts": {
    "docs:dev": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.docs.js",
    "docs:build": "cross-env NODE_ENV=production webpack --config ./build/webpack.docs.js",
    "index:build": "webpack --config ./build/webpack.all.js",
    "component:build": "webpack --config ./build/webpack.component.js && node ./build/write.js",
    "assets:build": "webpack --config ./build/webpack.assets.js",
    "build": "rimraf lib && npm run index:build && npm run component:build && npm run assets:build",
    "upload:docs": "npm run docs:build && node ./build/qiniu.js",
    "prepublishOnly": "npm run build && npm run upload:docs",
    "pub": "node ./build/publish.js",
    "lint": "eslint --ext .js,.vue packages src",
    "lint-fix": "eslint --fix --ext .js,.vue packages src",
    "lint:style": "stylelint **/*.{html,vue,css,sass,scss} --fix",
    "test:unit": "vue-cli-service test:unit --watch",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{html,vue,css,sass,scss}": [
      "npm run lint:style"
    ],
    "*.{js,ts,vue}": [
      "npm run lint"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/runtime-corejs3": "^7.12.5",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@vue/eslint-config-standard": "^6.0.0",
    "@vue/test-utils": "^1.1.2",
    "autoprefixer": "^9.7.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "core-js": "^3.8.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-vue": "^7.4.0",
    "eslint-webpack-plugin": "^2.4.1",
    "file-loader": "^6.2.0",
    "highlight.js": "^9.18.5",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "markdown-it": "^12.0.4",
    "markdown-it-anchor": "^6.0.1",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-container": "^3.0.0",
    "markdown-it-table-of-contents": "^0.5.1",
    "markdown-it-task-lists": "^2.1.1",
    "mini-css-extract-plugin": "^1.3.3",
    "node-sass": "^5.0.0",
    "postcss-loader": "^4.1.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "url-loader": "^4.1.1",
    "vue": "^2.6.12",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.9.6",
    "vue-router": "^3.4.9",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^5.7.3"
  },
  "peerDependencies": {
    "vue": "^2.6.12"
  },
  "dependencies": {
    "async-validator": "^3.5.1",
    "deepmerge": "^4.2.2",
    "flv.js": "^1.5.0",
    "hls.js": "^0.14.17",
    "lodash": "^4.17.20",
    "resize-observer-polyfill": "^1.5.1",
    "spark-md5": "^3.0.1"
  }
}
